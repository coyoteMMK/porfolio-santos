---
import ExperienceItem from "./ExperienceItem.astro";

const EXPERIENCIE = [
    {
        title: "Soporte TIC",
        date: "Actualmente...",
        company: "Dia Group",
        description:
            "Mantener y solucionar problemas con los sistemas tecnológicos que se utilizan para la gestión del almacén, como el software de gestión de almacén, los sistemas de identificación por radiofrecuencia (RFID) y otros dispositivos electrónicos. ",
        link: "#",
    },
];
---

<ol class="relative mt-16">
    {
        // Recorremos el array de experiencias y lo mostramos
        // con el componente ExperienceItem
        EXPERIENCIE.map((experiencie) => (
            <li class="">
                <ExperienceItem {...experiencie} />
            </li>
        ))
    }
</ol>
